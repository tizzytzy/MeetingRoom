<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
 
	<!-- Database Configuration -->
	
	<import resource="../database/Hibernate.xml"/>
 
<bean name="meetingRoomDao" class="com.meeting.persistence.impl.MeetingRoomDaoImpl">
<property name="hibernateTemplate" ref="hibernateTemplate" />

	</bean>
	<bean name="meetingRoomService" class="com.meeting.service.impl.MeetingRoomServiceImpl">
		<property name="meetingRoomDao" ref="meetingRoomDao" />
	</bean>
</beans>